package com.data.gabeli.sort;

import com.data.gabeli.search.DataItem;

public class SortItem implements Comparable<DataItem>{
	private int mKeyId;
	public String mName;
	private int mValue;


	private static int S_ID = 0;

	public SortItem(String aName,int aValue) {
		synchronized (DataItem.class) {
			mKeyId = S_ID;
			S_ID++;
		}
		mName = aName;
		mValue = aValue;
	}

	public int getmKeyId() {
		return mKeyId;
	}
	
	public int getmValue() {
		return mValue;
	}
	@Override
	public int compareTo(DataItem aTarget) {
		if (mKeyId < aTarget.getmValue()) // 这里比较的是什么 sort方法实现的就是按照此比较的东西从小到大排列
			return -1;
		if (mKeyId > aTarget.getmValue())
			return 1;
		return 0;
	}
}
