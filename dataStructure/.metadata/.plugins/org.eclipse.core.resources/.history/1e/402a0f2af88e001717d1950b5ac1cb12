package com.data.gabeli.search;


public class DataItem  implements Comparable<DataItem>{
	private int mKeyId;
	public String mName;

	private static int S_ID = 0;

	public DataItem(String aName) {
		synchronized (DataItem.class) {
			mKeyId = S_ID;
			S_ID++;
		}
		mName = aName;
	}

	public int getmKeyId() {
		return mKeyId;
	}
	
	@Override
	public int compareTo(DataItem arg0) {
		if (mKeyId < arg0.getmKeyId()) // 这里比较的是什么 sort方法实现的就是按照此比较的东西从小到大排列
			return -1;
		if (mKeyId > arg0.getmKeyId())
			return 1;
		return 0;
	}
}
